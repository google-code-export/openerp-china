<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="taobao_shop_refund_view_form" model="ir.ui.view">
            <field name="name">taobao_shop_refund_view_form</field>
            <field name="model">taobao.shop</field>
            <field name="inherit_id" ref="taobao.view_taobao_shop_form" />
            <field name="arch" type="xml">
                <page string="Basic" position="after">
                    <page string="Refund">
                        <field name="enable_auto_check_refund"/>
                        <newline />
                        <field name="refund_helpdesk_user_id"/>
                        <field name="refund_helpdesk_section_id"/>
                        <field name="refund_helpdesk_channel_id"/>
                        <field name="refund_helpdesk_priority"/>
                        <field name="refund_helpdesk_categ_id"/>
                        <field name="refund_remind_user"/>
                    </page>
                </page>
            </field>
        </record>

    </data>
</openerp>
